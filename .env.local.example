# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Gemini API
GEMINI_API_KEY=your_gemini_api_key

# Aimovely API
AIMOVELY_CLIENT_ID=your_aimovely_client_id
AIMOVELY_CLIENT_SECRET=your_aimovely_client_secret

# Qwen Hot Mode API (Keep this secret - do NOT commit the actual URL)
# This should point to your Qwen model endpoint
QWEN_API_URL=https://your-qwen-api-endpoint.com/sync_prompt

# FAL API (for Flux/Kontext Pro)
FAL_KEY=your_fal_key

# Google Cloud Platform (for Gemini Image)
# Default project ID: composite-silo-470413-r9
GOOGLE_CLOUD_PROJECT_ID=composite-silo-470413-r9
GOOGLE_CLOUD_LOCATION=global
# Optional: Override default model ID
GEMINI_IMAGE_MODEL_ID=gemini-3-pro-image-preview
# Note: SDK will use Application Default Credentials (ADC)
# Make sure to run: gcloud auth application-default login
